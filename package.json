{
	"name": "sort-line-length",
	"displayName": "Sort lines by their length",
	"description": "Easily sort imports and other arrays with a quick flick of the wrist!",
	"version": "0.0.1",
	"icon": "images/rootfoldername.png",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"author": {
		"name": "Leevashan Reddi",
		"email": "zombiebunny01@gmail.com"
	},
	"activationEvents": [
		"onCommand:extension.sortLineLength"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.sortLineLength",
				"title": "Sort lines"
			}
		],
		"keybindings": [
            {
                "command": "extension.sortLineLength",
                "key": "ctrl+alt+z",
                "mac": "shift+cmd+z"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": ""
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.1",
		"@types/vscode": "^1.39.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "~4.6.2",
		"vscode-test": "^1.3.0"
	}
}
