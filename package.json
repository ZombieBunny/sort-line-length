{
	"name": "sort-line-length",
	"displayName": "Sort lines by line length",
	"description": "Transform your messy code into a work of art with Sort Lines By Line Length, the ultimate tool for effortless organization and polished programming.",
	"version": "1.1.4",
	"icon": "images/sorted.png",
	"repository": "https://github.com/ZombieBunny/sort-line-length",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"author": {
		"name": "Leevashan Reddi",
		"email": "zombiebunny01@gmail.com"
	},
	"activationEvents": [
		"onCommand:extension.sortLineLength"
	],
	"main": "./out/extension.js",
	"publisher": "zombiebunny",
	"contributes": {
		"commands": [
			{
				"command": "extension.sortLineLength",
				"title": "Sort lines"
			}
		],
		"keybindings": [
			{
				"command": "extension.sortLineLength",
				"key": "ctrl+alt+l",
				"mac": "cmd+shift+l"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": ""
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.1",
		"@types/vscode": "^1.39.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "~4.6.2",
		"vscode-test": "^1.3.0"
	}
}
